function DeleteUserModule($user_id,$workspace_id,$module_id) {
	$db=$GLOBALS['db'];

	$sql="DELETE FROM ".$GLOBALS['mysql_db']."workspace_user_modules
				WHERE user_id = $user_id
				AND workspace_id = $workspace_id
				AND module_id = $module_id
				";
	$result = $db->Query($sql);

	if ($db->AffectedRows() > 0) {
		return True;
	}
	else {
		return False;
	}
}
